<!-- DataTales Example -->

<div class="tab-pane fade show active" id="show-image" role="tabpanel" aria-labelledby="home-tab">
    <div class="row">
        <% for (let i = 0; i < item.imageId.length; i++ ) { %>
            <div class="card shadow mb-4 mt-2 col-6">
                <div class="card-body">
                    <img src="http://localhost:3000/<%- item.imageId[i].imageUrl %>" alt="image" style="width: 100%; display: block;">
                </div>
            </div>
        <% } %>
    </div>
    <a href="/admin/item" class="btn btn-warning" >Back</a>
</div>